                </Tab>
                  <Tab style="-fx-background-color: #FFFFFF;" text="Categorie">
                     <content>
                        <fx:include source="page/PageCategorie.fxml" />
                     </content>
                  </Tab>
                  <Tab style="-fx-background-color: #FFFFFF;" text="Commande">
